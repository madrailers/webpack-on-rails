{
  "extends": "airbnb",
  "globals": {
    "document": true
  },
  "rules": {
    "no-multi-spaces": 0,
    "object-curly-spacing": 0,
    "space-before-function-paren": ["error", "always"],
    "arrow-body-style": 0,
    "react/no-array-index-key": 0,
    "no-console": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0
  }
}
